{
	"name": "Heroku aria2c",
	"description": "Run aria2c on heroku",
	"repository": "https://github.com/moeik/Heroku_aria2",
	"keywords": ["aria2c"],
	"env": {
		"HEROKU_APP_NAME": {
			"description": "This should be the same as \"App name\" above."
		},
		"ARIA2C_SECRET": {
			"description": "填写一个后台密码",
			"value": ""
		},
		"RCLONE_CONFIG": {
			"description": "每一行配置后面用\n替换换行符",
			"required": false
		},
		"RCLONE_DESTINATION": {
			"description": "填写一个OneDrive上传路径，如：/update",
			"required": false
		}
	},
	"buildpacks": [
		{
			"url": "heroku/nodejs"
		}
	]
}